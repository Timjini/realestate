<div id="<%= dom_id property %>">
  <p>
    <strong>Name:</strong>
    <%= property.name %>
  </p>

  <p>
    <strong>Street:</strong>
    <%= property.street %>
  </p>

  <p>
    <strong>City:</strong>
    <%= property.city %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= property.country %>
  </p>

  <p>
    <strong>Zipcode:</strong>
    <%= property.zipcode %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= property.price %>
  </p>

    <p>
    <strong>Photo:</strong>
    <%= image_tag "http://localhost:3000#{property.photo}" , :class => "estate-img" %>
  </p>

</div>
